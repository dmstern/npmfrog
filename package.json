{
  "name": "npmfrog",
  "version": "1.2.0-rc.1",
  "scripts": {
    "prod": "pm2 start pm2.prod.config.js",
    "dev": "npm run copy && pm2 start pm2.dev.config.js",
    "start": "npm run copy && pm2 start pm2.dev.config.js --env local",
    "ps": "pm2 l",
    "webui": "vue-cli-service serve",
    "server": "cd dist/server && nodemon .",
    "server:local": "cross-env MOCK=true npm run server",
    "copy": "cp-cli server/mock dist/server/mock",
    "lint": "vue-cli-service lint && tslint src/**/*.ts types/**/*.ts server/**/*.ts bin/*.ts",
    "prettier": "prettier src/**/* types/**/* server/**/* --write",
    "compile": "npm run lint && tsc bin/cli.ts && cd server && tsc",
    "compile:watch": "cd server && tsc --watch",
    "build": "npm run compile && npm run copy && vue-cli-service build --dest dist/webui",
    "logs": "pm2 logs /frog-.*/",
    "stop": "pm2 delete /frog-.*/",
    "pre-release": "release-it --preRelease=alpha",
    "release:rc": "release-it --preRelease=rc --npm.tag=next",
    "release:patch": "release-it",
    "release:minor": "release-it minor",
    "release:major": "release-it major",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@fortawesome/fontawesome-free": "^5.5.0",
    "@mdi/font": "^3.0.39",
    "axios": "^0.21.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "cross-spawn": "^6.0.5",
    "express": "^4.16.4",
    "express-serve-static-core": "^0.1.1",
    "fs-extra": "^7.0.1",
    "jsdom": "^15.2.0",
    "highlight.js": "^10.4.1",
    "mime": "^2.4.0",
    "morgan": "^1.9.1",
    "node-emoji": "^1.8.1",
    "parse-authors": "^0.2.4",
    "pm2": "^4.4.0",
    "showdown": "^1.9.1",
    "tar": "^4.4.8",
    "url-parse": "^1.4.5",
    "vue": "^2.5.17",
    "vue-class-component": "^6.3.2",
    "vue-clipboard2": "^0.2.1",
    "vue-highlightjs": "^1.3.3",
    "vue-property-decorator": "^7.2.0",
    "vue-router": "^3.0.2",
    "vue-simple-svg": "^1.3.2",
    "vue-timeago": "^5.0.0",
    "vuetify": "^1.3.11",
    "snyk": "^1.278.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "@types/cross-spawn": "^6.0.0",
    "@types/express": "^4.16.0",
    "@types/fs-extra": "^5.0.4",
    "@types/jsdom": "^12.2.0",
    "@types/morgan": "^1.7.35",
    "@types/node-emoji": "^1.8.0",
    "@types/showdown": "^1.7.5",
    "@types/tar": "^4.0.0",
    "@vue/cli-plugin-typescript": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "ajv": "^6.5.5",
    "concurrently": "^4.1.0",
    "cp-cli": "^1.1.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "husky": "^1.2.0",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.7",
    "pretty-quick": "^1.8.0",
    "release-it": "^15.9.0",
    "sass-loader": "^7.1.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.1",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.5.17"
  },
  "files": [
    "dist",
    "ecosystem.config.js",
    ".sample.config.json",
    "pm2.config.js",
    "pm2.prod.config.js",
    "bin/cli.js"
  ],
  "bin": {
    "npmfrog": "bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dmstern/npmfrog.git"
  },
  "bugs": {
    "url": "https://github.com/dmstern/npmfrog/issues",
    "email": "morgenstern.daniel@gmail.com"
  },
  "keywords": [
    "npm",
    "npmjs",
    "jfrog",
    "artifactory",
    "verdaccio",
    "jfrog-artifactory",
    "registry",
    "package-manager",
    "material-design",
    "npm-registry"
  ],
  "author": "Daniel Morgenstern <morgenstern.daniel@gmail.com>",
  "license": "ISC",
  "description": "A nice npmjs.org-like web interface for jFrog Artifactory",
  "main": "src/main.ts",
  "snyk": true
}
